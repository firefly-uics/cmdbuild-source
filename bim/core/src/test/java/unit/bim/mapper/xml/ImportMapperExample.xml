<bim-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="import-conf.xsd"> 
<entity name="IfcBuilding" label="Building">
	<attributes>
		<attribute type="simple" name="Name" label="Code" /> 
		<attribute type="simple" name="Name" label="Description" /> 
	</attributes>
</entity>
<entity name="IfcBuildingStorey" label="Floor">
	<attributes>
		<attribute type="simple" name="Name" label="Code" /> 
		<attribute type="simple" name="Name" label="Description" /> 
		<attribute type="multiple" name="Decomposes">
			<entity>
				<attributes>
					<attribute type="reference" name="RelatingObject">
						<entity>
							<attributes>
								<attribute type="simple" name="GlobalId" label="Building" />
							</attributes>
						</entity>
					</attribute>
				</attributes>
			</entity>
		</attribute>
	</attributes>
</entity>
<entity name="IfcSpace" label="Room">
	<attributes>
		<attribute type="simple" name="Name" label="Code" /> 
		<attribute type="simple" name="Name" label="Description" /> 
		<attribute type="simple" name="_Geometry" label="" />
		<attribute type="multiple" name="Decomposes">
			<entity>
				<attributes>
					<attribute type="reference" name="RelatingObject">
						<entity>
							<attributes>
								<attribute type="simple" name="GlobalId" label="Floor" />
							</attributes>
						</entity>
					</attribute>
				</attributes>
			</entity>
		</attribute>
	</attributes>
</entity>
